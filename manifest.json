{
  "manifest_version": 3,
  "name": "User-Agent Switcher",
  "version": "1.0",
  "description": "Cambia el User-Agent para dispositivos m√≥viles.",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "browsingData",
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}